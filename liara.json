{
  "app": "vteemo",
  "platform": "static",
  "path": "dist",
  "spa": true,
  "buildCommand": "npm run build:prod",
  "publicPath": "/",
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}